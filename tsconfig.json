{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2015",
    "rootDir": ".",
    "moduleResolution": "node",
    "outDir": "./bin",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
    "include": ["api/**/*"]
}
